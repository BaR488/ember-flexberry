/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Global */
@site       : 'crimea';
@reset      : 'crimea';

/* Elements */
@button     : 'crimea';
@container  : 'crimea';
@divider    : 'crimea';
@flag       : 'crimea';
@header     : 'crimea';
@icon       : 'crimea';
@image      : 'crimea';
@input      : 'crimea';
@label      : 'crimea';
@list       : 'crimea';
@loader     : 'crimea';
@rail       : 'crimea';
@reveal     : 'crimea';
@segment    : 'crimea';
@step       : 'crimea';

/* Collections */
@breadcrumb : 'crimea';
@form       : 'crimea';
@grid       : 'crimea';
@menu       : 'crimea';
@message    : 'crimea';
@table      : 'crimea';

/* Modules */
@accordion  : 'crimea';
@checkbox   : 'crimea';
@dimmer     : 'crimea';
@dropdown   : 'crimea';
@embed      : 'crimea';
@modal      : 'crimea';
@nag        : 'crimea';
@popup      : 'crimea';
@progress   : 'crimea';
@rating     : 'crimea';
@search     : 'crimea';
@shape      : 'crimea';
@sidebar    : 'crimea';
@sticky     : 'crimea';
@tab        : 'crimea';
@transition : 'crimea';

/* Views */
@ad         : 'crimea';
@card       : 'crimea';
@comment    : 'crimea';
@feed       : 'crimea';
@item       : 'crimea';
@statistic  : 'crimea';

/*******************************
            Folders
*******************************/

/* Path to theme packages */
@themesFolder : 'themes';

/* Path to site override folder */
@siteFolder  : 'site';


/*******************************
         Import Theme
*******************************/

@import "tests/dummy/app/styles/theme.less";

/* End Config */