/*******************************
         Theme Overrides
*******************************/
.ui.sidebar {
    position: absolute!important;
    top: 180px;
    float: left;
    box-shadow: none!important;
    border-radius: 5px !important;

}
.page-header-caption a{
    background: none;
    color: #fff;
    vertical-align: middle;
    text-decoration: none;
    font-size: 24px;
    border-radius: 0px;
    box-shadow: none;
}
.page-header-caption a:hover{
    color: #fff!important;
}
.page-header-caption {
    display: flex;
    align-items: center;
}
.ui.attached.menu {
    border-bottom: none!important;
}
.flex-container {
    width: 100%!important;
    justify-content: space-between;
}
.ui.top.attached {
    margin: 5px;
    border-radius: 5px!important;
}
.ui.left.sidebar {
    width: 260px;
}
.inverted.vertical.menu.sidebar {
    left: 0px!important;
    height: 75.7vh!important;

}
.ui .attached .menu {
    border: none!important;
}
.ui.attached.menu:not(.tabular) {
    border: none!important;
    border-radius: 5px;
}
.ui.menu .item:before {
    background: none;
}
.ui.menu:not(.vertical) .item {
    border-left: none!important;
}
#example .flexberry-content {
    border: none;
}
.ui.attached.menu > .flex-container > .item {
    padding:0px 30px!important;
}
.launch.icon.item:hover {
    background: none!important;
}
.launch.icon.item:hover:before {
    color: #fff!important;
}
.text_menu {
    margin-left:15px;
    color: #7ba7d9;
    font-size: 15px;
}
.hidden-text {
    color: #888!important;
}
.hidden-menu {
    display: none!important;
}