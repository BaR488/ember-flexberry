<h3 class="ui header">{{t "forms.components-examples.flexberry-lookup.limit-function-with-dinamic-properties-example.caption"}}</h3>
<form class="ui form flexberry-vertical-form" role="form">
  <div class="field">
    {{flexberry-lookup
      value=model.type
      projection="LookupWithLimitFunctionExampleView"
      displayAttributeName="name"
      title=(t "forms.components-examples.flexberry-lookup.limit-function-example.titleLookup")
      relationName="type"
      choose="showLookupDialog"
      remove="removeLookupValue"
      readonly=readonly
      autocomplete=true
      dynamicProperties=dynamicProperties
    }}
  </div>
</form>

<form class="ui form flexberry-vertical-form" role="form">
  <button class="ui button" {{action 'firstLimitFunction' on="click"}}>{{t "forms.components-examples.flexberry-lookup.limit-function-with-dinamic-properties-example.captionFirstLimitFunction"}}</button>
  <button class="ui button" {{action 'secondLimitFunction' on="click"}}>{{t "forms.components-examples.flexberry-lookup.limit-function-with-dinamic-properties-example.captionSecondLimitFunction"}}</button>
  <button class="ui button" {{action 'clearLimitFunction' on="click"}}>{{t "forms.components-examples.flexberry-lookup.limit-function-with-dinamic-properties-example.captionClearLimitFunction"}}</button>
</form>
