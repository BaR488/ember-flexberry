<h3 class="ui header">{{t "forms.components-examples.flexberry-textarea.settings-example.caption"}}</h3>
<form class="ui form flexberry-vertical-form" role="form">
  {{#settings-example
    controllerProperties=this
    componentSettingsMetadata=componentSettingsMetadata
    componentTemplateText=componentTemplateText
  }}
    {{flexberry-textarea
      value=model.text
      placeholder=placeholder
      readonly=readonly
      class=class
    }}
  {{/settings-example}}
</form>
