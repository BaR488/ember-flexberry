<div id="lookup-list" class="list-group">
  {{flexberry-objectlistview
    class="ui bottom attached"
    modelProjection=modelProjection
    content=model
    selectedRecord=currentLookupRow
    componentMode='lookupform'
    componentName='flexberry-objectlistview-at-lookup'

    filterByAnyMatch=(action 'filterByAnyMatch')
    filterText=filter

    customProperties=customPropertiesData
  }}
</div>
